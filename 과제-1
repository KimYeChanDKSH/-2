#include <stdio.h>
#include <stdlib.h>

int main() {
int a, b;
a = 50;
b = 100;
printf("a의 값은 %d이고 b의 값은 %d입니다", a, b);
return 0;
}

}
#include <stdio.h>
#include <stdlib.h>

int main() {
int i, j, k, m;
i = 10; j = 10;
k = i++; m = ++j;
printf("i = %d, k = %d \n", i, k);
printf("j = %d, k = %d \n", j, m);
return 0;
}
